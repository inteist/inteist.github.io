{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "layout",
          "name": "layout",
          "type": "string"
        },
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "last_modified_at",
          "name": "last_modified_at",
          "type": "datetime"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.content.pageFolders": [
    {
      "path": "[[workspace]]/_drafts",
      "title": "_drafts",
      "originalPath": "[[workspace]]/_drafts"
    },
    {
      "path": "[[workspace]]/_posts",
      "title": "_posts",
      "originalPath": "[[workspace]]/_posts"
    },
    {
      "path": "[[workspace]]/pages",
      "title": "pages",
      "originalPath": "[[workspace]]/pages"
    },
    {
      "path": "[[workspace]]/pages/pygment-samples"
    }
  ],
  "frontMatter.content.fmHighlight": true,
  "frontMatter.custom.scripts": [],
  "frontMatter.dashboard.openOnStart": false,
  "frontMatter.framework.startCommand": "jekyll s",
  "frontMatter.preview.host": "http://127.0.0.1:4000//"
}