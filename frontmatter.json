{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "previewPath": null,
      "pageBundle": false,
      "fields": [
        {
          "title": "layout",
          "name": "layout",
          "type": "string"
        },
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "summary",
          "name": "summary",
          "type": "datetime"
        },
        {
          "title": "last_modified_at",
          "name": "last_modified_at",
          "type": "datetime"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "datetime"
        },
        {
          "title": "keywords",
          "name": "keywords",
          "type": "choice",
          "choices": [
            "mogrify",
            "app store",
            "homebrew"
          ]
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        }
      ]
    }
  ]
}