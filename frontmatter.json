{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "layout",
          "name": "layout",
          "type": "string"
        },
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "last_modified_at",
          "name": "last_modified_at",
          "type": "datetime"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.content.pageFolders": [
    {
      "path": "[[workspace]]/pages"
    }
  ],
  "frontMatter.taxonomy.tags": [
    "*nix",
    "android",
    "code",
    "css",
    "dev",
    "enlightening",
    "flutter",
    "git",
    "iOs",
    "jekyll",
    "mobile",
    "osx",
    "poole",
    "pygments",
    "resources",
    "shell",
    "source",
    "technical",
    "web"
  ],
  "frontMatter.taxonomy.categories": [
    "page",
    "post",
    "source"
  ]
}